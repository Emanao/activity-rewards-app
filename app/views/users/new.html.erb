<h1>Signup</h1>
<%= form_for @user do |f| %>
<table>
    <tr><th><%= f.label :name %></th> <td><%= f.text_field :name %></td></tr>
    <tr><th><%= f.label :password %></th> <td><%= f.password_field :password%></td></tr>
    <tr><th><%= f.label :password_confirmation %></th> <td><%= f.password_field :password_confirmation%></td></tr>

</table>
<%= f.submit "Sign Up" %>
<% end %>